{
    "name": "wso",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@testing-library/jest-dom": "^5.12.0",
        "@testing-library/react": "^11.2.6",
        "@testing-library/user-event": "^12.8.3",
        "@types/jest": "^26.0.23",
        "@types/node": "^12.20.11",
        "@types/react": "^17.0.4",
        "@types/react-dom": "^17.0.3",
        "env-cmd": "^10.1.0",
        "formik": "^2.2.6",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-router": "^5.2.0",
        "react-router-dom": "^5.2.0",
        "react-scripts": "4.0.3",
        "typescript": "^4.2.4",
        "web-vitals": "^1.1.1"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "env-cmd -f ./.husky/.config react-scripts test",
        "eject": "react-scripts eject",
        "lint": "eslint \"**/*.{ts,tsx}\"",
        "prettier": "prettier --write \"src/**/*.{ts,tsx,css,html}\"",
        "prettier:check": "prettier --config .prettierrc --check \"src/**/*.{ts,tsx,css,html}\"",
        "husky": "lint-staged & npm run prettier & npm run prettier:check",
        "prepare": "husky install"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "lint-staged": {
        "src/**/*.{ts,tsx,css,html,json}": [
            "npm run prettier:check"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@types/react-router-dom": "^5.1.7",
        "@typescript-eslint/eslint-plugin": "^4.23.0",
        "@typescript-eslint/parser": "^4.23.0",
        "eslint": "^7.26.0",
        "eslint-plugin-react": "^7.23.2",
        "lint-staged": "^11.0.0",
        "prettier": "^2.3.0",
        "husky": "^6.0.0"
    }
}
